<h3>対象の一覧画面</h3>
<%= flash[:notice] %>

<table>
  <% @meeting_logs.each do |m| %>
    <tr>
      <td>image</td>
    </tr>
    <tr>
      <td><%= m.name %> さん</td>
    </tr>
    <tr>
      <td><%= m.position %></td>
    </tr>
    <tr>
      <td>#tag</td>
    </tr>
    <tr>
      <td>
        <%= m.status %>,
        <%= link_to "編集", edit_meeting_log_path(m.id) %> & 削除
      </td>
    </tr>
  <% end %>
</table>
