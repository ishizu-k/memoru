<%= render partial: "menu" %>

<div class="lists">
  <ul>
    <% @meeting_logs.each do |m| %>
      <div class="list-name">
        <table class="table-list">
          <tr><td><%= link_to (m.name), meeting_log_path(m.id) %> さん</td></tr>
          <tr><td>
            <details>
              <summary>画像を表示する</summary>
              <span class="image-name-only"><%= image_tag m.image.url %></span><br>
              <%= m.position %>
            </details>
          </td></tr>
          <tr><td></td></tr>
        </table>
      </div>
    <% end %>
  </ul>

  <%= paginate @meeting_logs %>
</div>
